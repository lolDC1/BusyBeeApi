{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AzureAdB2C": {
    "Instance": "https://{YOUR_TENANT_NAME e.g. myexplainthecloud}.b2clogin.com",
    "Domain": "{YOUR_TENANT_DOMAIN_NAME e.g. myexplainthecloud.onmicrosoft.com}",
    "ClientId": "{YOUR_BACKEND_CLIENT_ID}",
    "SignedOutCallbackPath": "/signout/B2C_1_SignupSignin",
    "SignUpSignInPolicyId": "B2C_1_SignupSignin",
    "OpenApiClientId": "{YOUR_FRONTEND_CLIENT_ID}",
    "OpenApiScopeApi": "{YOUR_API_SCOPE}"
  },
  "LocalStorage": {
    "ImagesUrlPrefix": "/images/",
    "ImagesFolderPath": "images",
    "FilesUrlPrefix": "/files/",
    "FilesFolderPath": "files",
    "CategoryIconFilenamePrefix": "category_icon_",
    "UserPhotoFilenamePrefix": "user_photo_",
    "PortfolioFilenamePrefix": "portfolio_"
  },
  "ConnectionStrings": {
    "DefaultConnection": "{YOUR_CONNECTION_STRING}"
  }
}
